{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "breadcrumbs.enabled": true,
  "editor.fontFamily": "Monaco, Menlo, 'Courier New', monospace",
  "editor.fontSize": 14,
  "editor.maxTokenizationLineLength": 50000,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "all",
  "editor.rulers": [
    120
  ],
  "editor.suggestSelection": "first",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact",
    "vue-html": "html"
  },
  "files.associations": {
    "*.anim": "json",
    "*.fire": "json",
    "*.meta": "json",
    "*.prefab": "json",
    "*.wxml": "handlebars",
    "*.wxss": "scss"
  },
  "files.exclude": {
    "**/*.pyc": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true
  },
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "keyboard.touchbar.ignored": [
    "workbench.action.debug.run"
  ],
  "nasc-touchbar.addCursorBelow": false,
  "nasc-touchbar.goToDefinition": false,
  "nasc-touchbar.jumpToBracket": true,
  "nasc-touchbar.peekDefinition": true,
  "nasc-touchbar.showCommands": false,
  "nasc-touchbar.showReferences": true,
  "pgFormatter.functionCase": "uppercase",
  "prettier.printWidth": 120,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "python.formatting.autopep8Args": [
    "--max-line-length=120",
    "--ignore=W191,E101,E111,E26"
  ],
  "python.formatting.provider": "yapf",
  "python.formatting.yapfArgs": [
    // "--style={based_on_style: pep8, use_tabs: true, column_limit: 40, split_before_logical_operator: false}"
  ],
  "python.linting.flake8Args": [
    // "--ignore=W191,E128"
    // "--ignore=W191,E101,E111,E5,E12,E26,C0330"
  ],
  "python.linting.flake8Enabled": true,
  "python.linting.pylintEnabled": false,
  "search.exclude": {
    "**/bower_components": true,
    "**/node_modules": true,
    "**/tmp": true
  },
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.scrollback": 200000,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.openFilesInNewWindow": "default",
  "window.title": "${activeEditorMedium}${separator}${rootName}",
  "window.zoomLevel": 0,
  "workbench.colorTheme": "Tomorrow Night",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.tabSizing": "shrink",
  "workbench.iconTheme": "material-icon-theme"
}